<%- include('partials/header') -%>
  <div class="content">
    <h1>New post</h1>
    <form class="" action="/post" method="post">
      <div class="form-item">
        <label for="title">Title</label>
        <input type="text" name="title" value="" autocomplete="off">
      </div>
      <div class="form-item">
        <label for="content">Post</label>
        <textarea name="content" rows="14" cols="80"></textarea>
      </div>
      <input class="compose-btn" type="submit" value="post">
    </form>
  </div>

<%- include('partials/footer') -%>
